Algoritmo sin_titulo
	//algoritmo que calcule el porcetaje de alumnos hombres y mujeres de un salon
	Definir nh Como Entero;
	Definir nm Como Entero;
	definir total como entero;
	definir ph como real;
	Definir pm como real;
	Escribir "ingrese el numero de nh";
	Leer  nh;
	Escribir "ingrese le numero de nm";
	Leer  nm;
	total= nh+nm;
	ph=(total * nh) / 100;
	pm=(total * nm) / 100;
	Escribir "poblacion nh",ph;
	Escribir "poblacion nm",pm;
FinAlgoritmo
