Algoritmo sin_titulo
	//algorimo que permita calcular el salario mensula de un trabajdor teniendo en cuenta los dias que a trabajado y el valor de cada dia
	Definir costo Como Real;
	Definir dias_trabajados Como Entero;
	Definir respuesta_de_multiplicacion Como Real;
	Escribir "los dias trabajados:";
	Leer dias_trabajados;
	Escribir "costo";
	leer costo;
	
	
	respuesta_de_multiplicacion= dias_trabajados * costo;
	Escribir"su sueldo es :  ",respuesta_de_multiplicacion;
FinAlgoritmo
